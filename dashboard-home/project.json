{
  "name": "dashboard-home",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "dashboard-home/src",
  "prefix": "black-pearl",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "dashboard-home/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "dashboard-home/**/*.ts",
          "dashboard-home/**/*.html"
        ]
      }
    }
  }
}
